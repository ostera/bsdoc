{
  "name": "bsdoc",
  "version": "6.0.2-alpha",
  "keywords": ["BuckleScript", "Odoc-driver", "documentation", "generation"],
  "author": "Leandro Ostera <leandro@ostera.io>",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildsInSource": "_build",
    "release": {
      "bin": ["bsdoc"],
      "includePackages": ["root", "@opam/odoc"]
    }
  },
  "scripts": {
    "test": "esy x bsdoc --version",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.4.0 < 3.6.0",
    "@opam/bos": "0.2.0",
    "@opam/cmdliner": "1.0.4",
    "@opam/dune": "2.0.1",
    "@opam/fmt": "0.8.8",
    "@opam/logs": "0.7.0",
    "@opam/odoc": "1.4.2",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@opam/odoc": "1.4.0"
  }
}
